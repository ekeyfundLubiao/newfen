            android:screenOrientation="portrait" />
        <activity
            android:name=".view.my.systemset.SystemSettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".test.CbTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.my.invitefamily.FamilyNumbersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.my.systemset.UpdateLoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.account.CompleteBaseInfoActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".view.dynamics.SendDynamicActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.living.ALivingActivity" />
        <activity android:name=".webview.WebviewActivity" />
        <activity android:name=".view.daily.LoginActivity"></activity>
        <activity android:name=".view.kindergarten.TeacherDetailActivity"></activity>
